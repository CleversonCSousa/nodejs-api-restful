{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "@typescript-eslint/no-unused-vars": [
    "error",
    {
      "vars": "all"
    }
  ],
  "semi": ["error", "always"],
  "quotes": ["error", "single"]
}
